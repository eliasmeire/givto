{
  "version": 2,
  "name": "givto",
  "routes": [
    {
      "src": "^/api/graphql",
      "dest": "projects/givto-api/graphql.ts"
    },
    {
      "src": "(.*)",
      "dest": "projects/givto-website"
    }
  ],
  "alias": "givto.app",
  "env": {
    "MONGODB_URI": "@givto-mongodb-conn-str"
  }
}
