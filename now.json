{
  "version": 2,
  "name": "givto",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "cache-control", "value": "no-cache, no-store, max-age=0" }
      ]
    }
  ],
  "env": {
    "MONGODB_URI": "@givto-mongodb-conn-str",
    "MONGODB_DB": "@mongodb-dbname",
    "MAILGUN_API_KEY": "@mailgun-api-key",
    "MAILGUN_API_BASE": "@mailgun-api-base",
    "JWT_SECRET_KEY": "@jwt-secret-key"
  }
}
